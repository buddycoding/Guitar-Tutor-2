<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.guitartutor"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.RECORD_AUDIO" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppThemeLight">
        <activity android:name="com.example.guitartutor.Migrate.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.guitartutor.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName="com.example.guitartutor.TunerActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.guitartutor.TunerActivity"/>
        </activity>



        <activity android:name="com.example.guitartutor.TunerActivity" />
        <activity android:name="com.example.guitartutor.Migrate.ChordsSoundsActivity" />
        <activity android:name="com.example.guitartutor.Migrate.SongsChordsActivity" />
        <activity
            android:name="com.example.guitartutor.Migrate.QuestionsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.guitartutor.Migrate.SongsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.guitartutor.Migrate.GuitarTutorialActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.guitartutor.Migrate.ChordSheetActivity"
            android:screenOrientation="portrait" />

        <activity android:name="com.example.guitartutor.Migrate.TutorialGuitarStrumming"></activity>
        <activity android:name="com.example.guitartutor.Migrate.TutorialGuitarHolding" />
        <activity android:name="com.example.guitartutor.Migrate.TutorialGuitarString" />
        <activity android:name="com.example.guitartutor.Migrate.TutorialGuitarParts" />


    </application>

</manifest>